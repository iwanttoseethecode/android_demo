<resources>
    <declare-styleable name="BollMoveView">
        <attr name="exampleString" format="string" />
        <attr name="exampleDimension" format="dimension" />
        <attr name="exampleColor" format="color" />
        <attr name="exampleDrawable" format="color|reference" />
    </declare-styleable>

    <declare-styleable name="FistView">
        <attr name="text" format="string"/>
    </declare-styleable>

    <declare-styleable name="CustomProgressBar">
        <attr name="roundProgressColor" format="color"></attr>
        <attr name="roundColor" format="color"></attr>
        <attr name="roundWidth" format="dimension"></attr>
        <attr name="textSize" format="dimension"></attr>
        <attr name="textColor" format="color"></attr>
        <attr name="max" format="integer"></attr>
        <attr name="textShow" format="boolean"></attr>
    </declare-styleable>

    <declare-styleable name="RadarView">
        <attr name="startColor" format="color"/>
        <attr name="endColor" format="color"/>
        <attr name="backgroundColor" format="color"/>
        <attr name="circleNum" format="integer"/>
        <attr name="lineColor" format="color"/>
    </declare-styleable>
</resources>
